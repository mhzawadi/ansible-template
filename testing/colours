# Define some colors first:
#echo -e ""

export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

red='\x1b[0;31m'
RED='\x1b[1;31m'
green='\x1b[0;32m'
GREEN='\x1b[1;32m'
brown='\x1b[0;33m'
BROWN='\x1b[1;33m'
blue='\x1b[0;34m'
BLUE='\x1b[1;34m'
purple='\x1b[0;35m'
PURPLE='\x1b[1;35m'
cyan='\x1b[0;36m'
CYAN='\x1b[1;36m'
NC='\x1b[0m' # No Color
UL="\x1b[4m" # Under Line

